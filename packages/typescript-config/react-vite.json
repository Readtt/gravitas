{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React (Vite)",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "moduleDetection": "force",
    "noEmit": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "types": ["vite/client"]
  }
}